<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<HBox alignment="CENTER" prefHeight="35.0" styleClass="secondary-bg" stylesheets="@../main.css" VBox.vgrow="NEVER" xmlns="http://javafx.com/javafx/24" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.jzargo.buysmartgui.ui.template.FooterController">
        <children>
            <HBox alignment="CENTER_LEFT" styleClass="f" HBox.hgrow="ALWAYS">
                <children>
                    <Hyperlink onAction="#showCustomerService" styleClass="categories" text="Customer service">
                        <HBox.margin>
                            <Insets left="10.0" right="5.0" />
                        </HBox.margin>
                        <font>
                            <Font size="8.0" />
                        </font>
                    </Hyperlink>
                    <Hyperlink onAction="#showPrivacyPolicy" styleClass="categories" text="Privacy Policy">
                        <HBox.margin>
                            <Insets left="5.0" right="5.0" />
                        </HBox.margin>
                        <font>
                            <Font size="8.0" />
                        </font>
                    </Hyperlink>
                    <Hyperlink onAction="#showTerms" styleClass="categories" text="Terms of Use">
                        <HBox.margin>
                            <Insets left="5.0" right="5.0" />
                        </HBox.margin>
                        <font>
                            <Font size="8.0" />
                        </font>
                    </Hyperlink>
                </children>
            </HBox>
        </children>
        <VBox.margin>
            <Insets />
        </VBox.margin>
    </HBox>
