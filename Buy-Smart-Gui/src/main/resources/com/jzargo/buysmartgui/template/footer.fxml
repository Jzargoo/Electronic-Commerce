<HBox alignment="CENTER" prefHeight="35.0" styleClass="secondary-bg" stylesheets="@../main.css" VBox.vgrow="NEVER" xmlns="http://javafx.com/javafx/24" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.jzargo.buysmartgui.ui.FooterController">
